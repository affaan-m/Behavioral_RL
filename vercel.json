{
    "version": 2,
    "builds": [
        {
            "src": "src/web_igt.py",
            "use": "@vercel/python@3.1.0"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/web_igt.py"
        }
    ],
    "env": {
        "PYTHONPATH": "src",
        "PYTHON_VERSION": "3.11"
    }
} 