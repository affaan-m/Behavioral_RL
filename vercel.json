{
    "version": 2,
    "builds": [
        {
            "src": "src/web_igt.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/web_igt.py"
        }
    ],
    "env": {
        "PYTHONPATH": "src"
    }
} 